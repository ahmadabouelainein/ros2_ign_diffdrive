<?xml version="1.0"?>
<robot name="forklift_robot" xmlns:xacro="http://ros.org/wiki/xacro">
  <material name="Black">
    <color rgba="0 0 0 1" />
  </material>
  <material name="Grey">
    <color rgba="0.8 0.8 0.8 1" />
  </material>
  <material name="Teal">
    <color rgba="0 0.5 0.5 1" />
  </material>
  <link name="base_link" />
  <link name="chassis">
    <visual>
      <origin xyz="-1.0795 -0.5334 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/base_link.dae" />
      </geometry>
      <material name="Teal" />
    </visual>
    <collision>
      <origin xyz="-1.0795 -0.5334 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/base_link.dae" />
      </geometry>
    </collision>
    <inertial>
      <mass value="100" />
      <inertia ixx="10.4" ixy="0.0" ixz="0.0" iyy="35.4" iyz="0.0" izz="41.66" />
    </inertial>
  </link>
  <joint name="base_link_to_chassis" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.1" />
    <parent link="base_link" />
    <child link="chassis" />
  </joint>
  <link name="fork">
    <visual>
      <origin xyz="0 -0.368600 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/fork.dae" />
      </geometry>
      <material name="Grey" />
    </visual>
    <collision>
      <origin xyz="0 -0.368600 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/fork.dae" />
      </geometry>
    </collision>
    <inertial>
      <mass value="10" />
      <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.3" iyz="0" izz="0.4" />
    </inertial>

  </link>

  <joint name="fork_joint" type="prismatic">
    <origin rpy="0 0 0" xyz="1.0795 0 0.01" />
    <parent link="chassis" />
    <child link="fork" />
    <limit
      lower="0"
      upper="2"
      velocity="0.1"
      effort="100"
    />
    <axis xyz="0 0 1" />
    <dynamics damping="0.2" />
  </joint>

  <link name="wheel_back_left">
    <visual>
      <!-- <origin xyz="-0.2 0.2 -0.1" rpy="1.5707 0 0" /> -->
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/wheel.dae" />
      </geometry>
      <material name="Black" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.2" length="0.2" />
      </geometry>
    </collision>
    <inertial>
      <mass value="11.3" />
      <inertia ixx="0.26" ixy="0.0" ixz="0.0" iyy="0.26" iyz="0.0" izz="0.51" />
    </inertial>
    <surface>
      <friction>
        <ode>
          <mu>1.0</mu>  <!-- Friction coefficient -->
          <mu2>1.0</mu2>
        </ode>
      </friction>
    </surface>

  </link>

  <joint name="rear_left_wheel_joint" type="continuous">
    <origin xyz="-0.7795 -0.3835 0.1" rpy="1.57 0 0" />
    <parent link="chassis" />
    <child link="wheel_back_left" />
    <axis xyz="0 0 1" />
    <dynamics damping="0.2" />
  </joint>

  <link name="wheel_back_right">
    <visual>
      <!-- <origin xyz="-0.2 0.2 -0.1" rpy="1.5707 0 0" /> -->
      <geometry>
        <mesh filename="file://$(find ros2_ign_p2p_controllers)/model/wheel.dae" />
      </geometry>
      <material name="Black" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.2" length="0.2" />
      </geometry>
    </collision>
    <inertial>
      <mass value="11.3" />
      <inertia ixx="0.26" ixy="0.0" ixz="0.0" iyy="0.26" iyz="0.0" izz="0.51" />
    </inertial>
    <surface>
      <friction>
        <ode>
          <mu>1.0</mu>  <!-- Friction coefficient -->
          <mu2>1.0</mu2>
        </ode>
      </friction>
    </surface>

  </link>


  <joint name="rear_right_wheel_joint" type="continuous">
    <origin xyz="-0.7795 0.3835 0.1" rpy="1.57 0 0" />
    <parent link="chassis" />
    <child link="wheel_back_right" />
    <axis xyz="0 0 1" />
    <dynamics damping="0.2" />
  </joint>
